{
    "eslint.codeActionsOnSave.mode": "all",
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "eslint.debug": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
    },
    "files.eol": "\n",
    "editor.tabSize": 2
}